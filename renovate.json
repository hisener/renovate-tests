{
  "extends": [
    "config:base"
  ],
  "commitMessageAction": "Upgrade",
  "commitMessageTopic": "{{depName}}",
  "commitMessageExtra": "{{fromVersion}} -> {{toVersion}}",
  "packageRules": [
    {
      "datasources": [ "maven" ],
      "managers": [ "maven" ],
      "fileMatch": [ "^(?!(?:submodule-test\/)?pom\\.xml$).+" ],
      "enabled": false
    }
  ]
}
