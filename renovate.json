{
  "extends": [
    "config:base"
  ],
  "commitMessageAction": "Upgrade",
  "commitMessageTopic": "{{depName}}",
  "commitMessageExtra": "{{fromVersion}} -> {{toVersion}}",
  "enabledManagers": [
    "maven"
  ],
  "includePaths": [
    "pom.xml",
    "submodule-test/pom.xml"
  ],
  "packageRules": [
    {
      "packagePatterns": [
        "^com.google.(guava|auto)(\\..+)*:.+$"
      ],
      "versioning": "regex:^(?<major>\\d+)(\\.(?<minor>\\d+))?(\\.(?<patch>\\d+))?(-(?<compatibility>.*))?$",
      "allowedVersions": "!/.+-android/"
    }
  ]
}
